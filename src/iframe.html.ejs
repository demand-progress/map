<!doctype html>
<html lang="en">
<head>
  <title>Team Internet Event Map</title>

  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="application-name" content="Team Internet">
  <meta name="description" content="The FCC wants to destroy net neutrality and give giant cable companies control over the Internet.">
  <meta property="og:image" content="https://s3.us-east-2.amazonaws.com/teaminternet-map-prod/images/team-internet.jpg">
  <meta property="og:url" content="https://events.battleforthenet.com/">
  <meta property="og:title" content="Join a Team Internet Event">
  <meta property="og:description" content="The FCC wants to destroy net neutrality and give giant cable companies control over the Internet.">
  <meta property="og:site_name" content="events.battleforthenet.com">
  <meta name="twitter:card" value="summary_large_image">
  <!-- <meta name="twitter:site" content="@ACLU">
  <meta name="twitter:creator" content="@ACLU">
 -->

  <link rel="shortcut icon" href="<%= htmlWebpackPlugin.files.favicon %>">

  <!-- Include style sheets generated by webpack -->
  <% for (var css in htmlWebpackPlugin.files.css) { %>
  <link href="<%= htmlWebpackPlugin.files.css[css] %>" rel="stylesheet">
  <% } %>
</head>

<body>
  <%= require('src/templates/OutdatedBrowser.html') %>
  <div id="app" class="layout">
    <div class="layout-main">
      <%= require('src/templates/Iframe.html') %>
    </div>
  </div>
  <script src="<%= htmlWebpackPlugin.files.chunks['initialize'].entry %>"></script>
</body>
</html>
